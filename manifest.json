{
    "name": "nCage+",
    "options_page": "options.html",
    "permissions": ["storage"],
    "description": "Executes on load",
    "version": "0.1",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "image_replacer.js",
                "string_replacer.js"
            ],
            "run_at": "document_end"
        }
    ]    
}
